extends layout

block content
  h1= title
  - var image_names = ["chocolate.jpg", "pistachio.jpg", "redvelvet.jpg", "strawberry.jpg", "vanilla.jpg"]
  - for (let j = 0; j < 3; j++)
    - let rand = Math.floor(Math.random() * 5)
    img(src=`/images/${image_names[rand]}` alt=`${rand}` height="300px" width="300px")